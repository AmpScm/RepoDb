<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Title>RepoDb.MySqlConnector</Title>
    <TargetFrameworks>net10.0;net9.0;net8.0;netstandard2.0</TargetFrameworks>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Description>A hybrid .NET ORM library for MySQL (using MySqlConnector).</Description>
    <PackageTags>orm hybrid-orm micro-orm mysql mysqlconnector</PackageTags>
    <PackageReleaseNotes>https://repodb.net/release/mysqlconnector</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/AmpScm/RepoDB/tree/master/src/RepoDb.MySqlConnector/</PackageProjectUrl>
    <PackageId>$(PackagePrefix)$(AssemblyName)</PackageId>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MySqlConnector" />
  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\RepoDb\Compat.cs" />
    <None Include="README.md" Pack="True" PackagePath="\" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\RepoDb\RepoDb.csproj" />
  </ItemGroup>
</Project>
